        <!--<td id="k-v05"  text="通話結束" ><%# (body.charging_end_time  ).substr(-9) %></td>-->
        <td id="k-v06"  text="被叫號碼" style="color: #0055cc"><%= (body.called_number!=null ? body.called_number : "" ) %></td>

        <td id="k-v07"  text="＿＿IMEI" ><%= (body.calling_imei!=null ? body.calling_imei : "" ) +'\n'%>
