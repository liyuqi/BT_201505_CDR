        <td id="k-v11"  text="＿結束碼" ><%= (body.cause_for_termination!=null ? body.cause_for_termination : "") %></td>
        <td id="k-v12"  text="通話時間" ><%= (body.orig_mcz_duration!=null ? body.orig_mcz_duration : "" ) +'\n'%>
            <p style="color: #0055cc">
            <%=(body.term_mcz_duration!=null ? body.term_mcz_duration : "") %>
            </p>
        </td>

    </tr>
